#include <bits/stdc++.h>
using namespace std;

// Iterative + Constant Space DP
int main() {
#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
#endif

    int n;
    cin >> n;

    // Base cases
    if (n == 0) {
        cout << 0;
        return 0;
    }

    int prev2 = 0; // F(0)
    int prev = 1;  // F(1)

    for (int i = 2; i <= n; i++) {
        int cur = prev + prev2;
        prev2 = prev;
        prev = cur;
    }

    cout << prev;
    return 0;
}
